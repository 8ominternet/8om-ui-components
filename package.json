{
  "name": "8om-ui-components",
  "version": "1.0.0",
  "description": "UI Components for 8om Enterprise Applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 120 --no-semi --single-quote --jsx-bracket-same-line --parser flow --write",
      "git add"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-notes": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-eslint": "^8.2.2",
    "create-nwb-webpack-config": "^1.0.0",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nwb": "0.21.x",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "author": "",
  "homepage": "https://8om.io",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ]
}